{
  "author": "Vineet",
  "dark": true,
  "name": "Torva",
  "editorScheme": "/theme/mytheme.xml",
  "colors": {
    "primaryBackground": "#1a1d22",
    "primaryForeground": "#B9B9B9",
    "secondaryBackground": "#212121",
    "borderColor": "#35393F"
  },
  "ui": {
    "*": {
      "background": "primaryBackground",
      "foreground": "primaryForeground",
      "disabledBackground": "#212322",
      "inactiveBackground": "#212322",
      "selectionBackground": "#2d3139",
      "selectionInactiveBackground": "#2d333b"

    },
    "Borders": {
      "ContrastBorderColor": "primaryBackground",
      "color": "#212322"
    },
    "EditorTabs": {
      "background": "primaryBackground",
      "borderColor": "primaryBackground",
      "underlineHeight": 2,
      "underlineColor": "#497eb2",
      "underlinedTabBackground": "primaryBackground",
      "hoverBackground": "#212322"
    },
    "StatusBar.borderColor": "primaryBackground",
    "MenuBar.borderColor": "primaryBackground",
    "Tree": {
      "selectionInactiveBackground": "primaryBackground"
    }
  }
}